{
  "name": "@teleskop150750/resumable",
  "type": "module",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@7.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/teleskop150750/resumable.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "unbuild",
    "build:example": "tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm run prettier:fix && pnpm run lint:js:fix",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "lint:js": "eslint \"**/*.{ts,cts,js,cjs,json}\"",
    "lint:js:fix": "eslint --fix \"**/*.{ts,cts,js,cjs,json}\"",
    "commit": "cz",
    "prepare": "husky install && pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "axios": "^1.2.1",
    "bumpp": "^8.2.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-yml": "^1.2.0",
    "husky": "^8.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-md5": "^1.3.1",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vite": "^3.2.4",
    "yaml-eslint-parser": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
